# Bilhetinho API Environment Variables
# Copy this file to .env and update with your actual values

# Master Account Credentials
# IMPORTANT: Change these in production!
MASTER_USERNAME=master
MASTER_PASSWORD=change-this-to-a-strong-password

# JWT Configuration
# CRITICAL: Generate a strong random secret key for production
# Use: python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET_KEY=change-this-to-a-random-secret-key-in-production

# API Configuration
API_HOST=0.0.0.0
API_PORT=8001
ENVIRONMENT=development

# Database (if using PostgreSQL in production)
# DATABASE_URL=postgresql://user:password@localhost:5432/bilhetinho
